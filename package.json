  "bugs": {
    "url": "https://github.com/code-star/nope/issues"
  },
  "homepage": "https://github.com/code-star/nope#readme",
  "devDependencies": {
    "@types/jest": "^24.0.16",
    "@types/node": "^12.6.9",
    "jest": "^24.8.0",
    "typescript": "^3.5.3"
  }
}
