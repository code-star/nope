{
  "name": "nope",
  "version": "1.0.0",
  "description": "Applicative validation",
  "main": "index.js",
  "scripts": {
    "compile-test": "npm run clean && tsc -p . --p tsconfig.test.json",
    "test": "npm run compile-test && jest --no-cache",
    "coverage": "npm run compile-test && jest --no-cache --coverage",
